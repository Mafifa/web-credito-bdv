{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./src/",
    "paths": {
      "@images": [
        "images/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@styles/*": [
        "styles/*"
      ]
    }
  }
}